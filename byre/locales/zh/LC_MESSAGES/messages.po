#, python-brace-format
msgid "{value} is not in the range {range}."
msgstr "{value} 不在范围 {range} 中。"

#: click/types.py:606
msgid "{value!r} is not a valid boolean."
msgstr "{value!r} 不是合法的布尔值。"

